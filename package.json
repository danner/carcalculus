{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --modern",
    "test": "run-s test:unit test:e2e",
    "lint": "run-s lint:all:*",
    "build:ci": "yarn build --report",
    "dev": "vue-cli-service serve",
    "dev:e2e": "vue-cli-service test:e2e --mode=development",
    "docs": "vuepress dev",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:markdownlint": "yarn lint:markdownlint docs/*.md *.md",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,md}\"",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "new": "hygen new",
    "test:ci": "run-s test:unit:ci test:e2e",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.13",
    "@fortawesome/free-solid-svg-icons": "5.7.0",
    "@fortawesome/vue-fontawesome": "0.1.5",
    "axios": "0.18.0",
    "chart.js": "^2.7.3",
    "lodash": "4.17.11",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "vue": "2.5.22",
    "vue-chartjs": "^3.4.0",
    "vue-meta": "1.5.8",
    "vue-router": "3.0.2",
    "vuetify": "^1.4.3",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.3.0",
    "@vue/cli-plugin-e2e-cypress": "3.3.0",
    "@vue/cli-plugin-eslint": "3.3.0",
    "@vue/cli-plugin-unit-jest": "3.3.0",
    "@vue/cli-service": "3.3.1",
    "@vue/eslint-config-prettier": "4.0.1",
    "@vue/eslint-config-standard": "4.0.x",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.0.0",
    "cross-env": "5.2.x",
    "eslint-plugin-vue": "5.1.0",
    "express": "4.16.x",
    "hygen": "2.1.1",
    "imagemin-lint-staged": "0.4.0",
    "lint-staged": "8.1.1",
    "markdownlint-cli": "0.13.x",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "sass-loader": "7.1.x",
    "stylelint": "9.10.1",
    "stylelint-config-css-modules": "1.3.x",
    "stylelint-config-prettier": "4.0.x",
    "stylelint-config-recess-order": "2.0.1",
    "stylelint-config-standard": "18.2.x",
    "stylelint-scss": "3.5.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-jest": "3.0.2",
    "vue-template-compiler": "2.5.22",
    "vuepress": "0.14.8",
    "vuetify-loader": "^1.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  }
}
